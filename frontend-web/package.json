{
  "name": "ailivechallenge-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for AiLiveChallenge",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "axios": "^1.5.0",
    "socket.io-client": "^4.6.0"
  },
  "scripts": {
    "start": "DANGEROUSLY_DISABLE_HOST_CHECK=true BROWSER=none HOST=0.0.0.0 react-scripts start",
    "start:proxy": "DANGEROUSLY_DISABLE_HOST_CHECK=true BROWSER=none HOST=0.0.0.0 react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:4000",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
