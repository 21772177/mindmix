{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "frontend-web/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/api{,/**}", "function": "api" },
      { "source": "**", "function": "api" }
    ]
  }
}


